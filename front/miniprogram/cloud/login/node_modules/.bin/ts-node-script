#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../ts-node/dist/bin-script.js" "$@"
  ret=$?
else 
  node  "$basedir/../ts-node/dist/bin-script.js" "$@"
  ret=$?
fi
exit $ret
