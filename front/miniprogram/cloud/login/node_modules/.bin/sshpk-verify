#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../sshpk/bin/sshpk-verify" "$@"
  ret=$?
else 
  node  "$basedir/../sshpk/bin/sshpk-verify" "$@"
  ret=$?
fi
exit $ret
